{
    "routes": [
      {
        "route": "/*",
        "rewrite": "/index.html"
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html"
      }
    },
    "mimeTypes": {
      ".json": "application/json",
      ".html": "text/html",
      ".js": "application/javascript",
      ".css": "text/css",
      ".png": "image/png",
      ".jpg": "image/jpeg",
      ".gif": "image/gif",
      ".svg": "image/svg+xml"
    }
  }
  